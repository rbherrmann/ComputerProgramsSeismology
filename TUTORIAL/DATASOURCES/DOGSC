rm -f distlist
while read STA STLA STLO  ON OFF
do
DIST=`udelaz -ELAT ${EVLA} -ELON ${EVLO} -SLAT ${STLA} -SLON ${STLO} -DELKM`
AZ=`udelaz -ELAT ${EVLA} -ELON ${EVLO} -SLAT ${STLA} -SLON ${STLO}  -AZ`
echo $STA $DIST $AZ $ON $OFF >> distlist

done < gscsta

sort -n -k2 distlist | grep 29991231.0000 | uniq | awk '{ printf "%-6s %10.4f %5.0f %s %s\n",$1,$2,$3,$4,$5}' > gsc.list
fi
rm -f distlist

echo The output for the location $1 $2 is in the file gsc.list

rm -f gscsta
